{
  "name": "Superconnect",
  "description": "Helps you build a solid network",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*",
        "https://inbox.google.com/*"
      ],
      "js": [
        "inboxsdk.js",
        "main.js",
        "runtime.js",
        "chunk.js"
      ],
      "css": [
        "main.css"
      ],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    "../sapling.png"
  ],
  "permissions": [
    "https://mail.google.com/",
    "https://inbox.google.com/"
  ],
  "manifest_version": 2
}